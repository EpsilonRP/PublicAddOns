<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\..\FrameXML\UI.xsd">

	<Button name="EpsilonAuraManagerSearchSMTemplate" enableMouse="true" parentArray="searchPreview" virtual="true">
		<Size x="275" y="27"/>
		<Layers>
			<Layer level="OVERLAY" textureSubLevel="3">
				<Texture parentKey="selectedTexture" atlas="search-highlight" hidden="true">
					<Anchors>
						<Anchor point="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture name="$parentIcon" parentKey="icon">
					<Size x="21" y="21"/>
					<Anchors>
						<Anchor point="LEFT" x="5" y="1"/>
					</Anchors>
				</Texture>
				<FontString name="$parentSpellID" inherits="GameFontNormalSmall" justifyH="RIGHT" parentKey="spellID">
					<Anchors>
						<Anchor point="RIGHT" x="-5" y="0"/>
					</Anchors>
					<Color r="0.5" g="0.5" b="0.5" a="1"/>
				</FontString>
				<FontString name="$parentName" inherits="GameFontNormalSmall" justifyH="LEFT" parentKey="name">
					<Size x="400" y="0"/>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentIcon" relativePoint="RIGHT" x="5" y="0"/>
						<Anchor point="RIGHT" relativeTo="$parentSpellID" relativePoint="LEFT" x="-5" y="0"/>
					</Anchors>
					<Color r="1" g="1" b="1" a="1"/>
				</FontString>
			</Layer>
		</Layers>
		<NormalTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures_Tile" horizTile="true">
			<Size x="64" y="27"/>
			<TexCoords left="0.00000000" right="1.00000000" top="0.16601563" bottom="0.21875000"/>
		</NormalTexture>
		<PushedTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures_Tile" horizTile="true">
			<Size x="64" y="27"/>
			<TexCoords left="0.00000000" right="1.00000000" top="0.16601563" bottom="0.21875000"/>
		</PushedTexture>
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, "ANCHOR_LEFT");
				GameTooltip:SetSpellByID(self.SpellID)
				GameTooltip:Show();
				EpsilonAuraManagerAuraEditor_SetSearchPreviewSelection(self:GetID());
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnShow>
				self:SetFrameLevel(self:GetParent():GetFrameLevel() + 10);
			</OnShow>
			<OnClick>
				EpsilonAuraManagerAuraEditor_SelectSearch(self:GetID())
				EpsilonAuraManagerAuraEditor_ResetSearch()
			</OnClick>
		</Scripts>
	</Button>

