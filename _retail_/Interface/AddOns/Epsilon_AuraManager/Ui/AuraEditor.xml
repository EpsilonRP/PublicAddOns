		  </Layer>
		</Layers>
		<NormalTexture inherits="_SearchBarLg">
		</NormalTexture>
		<PushedTexture inherits="_SearchBarLg">
		</PushedTexture>
		<HighlightTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures">
			<Size x="256" y="47"/>
			<TexCoords left="0.00195313" right="0.50195313" top="0.80468750" bottom="0.85058594"/>
		</HighlightTexture>
		<Scripts>
		  <OnEnter>
			if self.spellID then
			  GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
			  GameTooltip:SetSpellByID(self.spellID)
			  self.showingTooltip = true;
			end
		  </OnEnter>
		  <OnLeave>
			GameTooltip:Hide();
			self.showingTooltip = false;
		  </OnLeave>
		  <OnClick>
			EpsilonAuraManagerAuraEditor_SelectSearch(self:GetID());
			PlaySound(SOUNDKIT.IG_SPELLBOOK_OPEN);
		  </OnClick>
		</Scripts>
	</Button>

