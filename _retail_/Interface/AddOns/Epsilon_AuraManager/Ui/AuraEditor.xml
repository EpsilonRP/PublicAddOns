		</EditBox>

	  <Button name="$parentBuffIconButton" parentKey="buffIcon" >
        <Size x="42" y="42"/>
		<NormalTexture parentKey="icon" setAllPoints="true" file="Interface/Icons/inv_misc_questionmark">
      	</NormalTexture>
		<DisabledTexture setAllPoints="true" desaturated="true" file="Interface/Icons/inv_misc_questionmark">
      	</DisabledTexture>
		<HighlightTexture parentKey="highlight" file="Interface/Addons/DiceMaster/Texture/trait-select">
		    <Size x="60" y="60"/>
          	<Anchors>
            	<Anchor point="CENTER"/>
			</Anchors>
		</HighlightTexture>
		<Anchors>
			<Anchor point="LEFT" x="20" y="0"/>
			<Anchor point="TOP" relativeKey="$parent.Header2" relativePoint="BOTTOM" x="0" y="-12"/>
		</Anchors>
        <Scripts>
          <OnClick>
            PlaySound(54132)
			EpsilonAuraManager_PickIcon()
          </OnClick>
		  <OnEnter>
			GameTooltip:SetOwner( self, "ANCHOR_RIGHT" )
			GameTooltip:ClearLines()
			GameTooltip:AddLine( "|cFFFFD100Click to select an icon for this aura.", 1, 1, 1, true )
			GameTooltip:Show()
		  </OnEnter>
		  <OnLeave>
		  	GameTooltip:ClearLines()
			GameTooltip:Hide()
		  </OnLeave>
        </Scripts>
      </Button>

	  <EditBox name="$parentBuffName" parentKey="buffName" font="GameFontNormalMed2" inherits="InputBoxTemplate">
		<Size x="200" y="12"/>
		<Anchors>
		  <Anchor point="TOPLEFT" relativeKey="$parent.buffIcon" relativePoint="RIGHT" x="16" y="0"/>
		</Anchors>
		<Layers>
		  <Layer level="ARTWORK">
			<FontString parentKey="Label" name="$parentBuffNameTitle" inherits="GameFontNormalLeft" justifyH="left" text="Name">
				<Size x="150" y="12"/>
				<Anchors>
				  <Anchor point="TOPLEFT" x="0" y="20"/>
				</Anchors>
			</FontString>
		  </Layer>
		</Layers>
		<Scripts>
		  <OnLoad>
			self:SetAutoFocus( false )
			self:SetTextColor( 1, 1, 1 )
			self:SetMaxLetters( 200 )
		  </OnLoad>
		  <OnEscapePressed function="EditBox_ClearFocus"/>
		  <OnEnterPressed function="EditBox_ClearFocus"/>
		  <OnTabPressed>
			EpsilonAuraManagerAuraEditorBuffDesc.EditBox:SetFocus();
		  </OnTabPressed>
		  <OnTextChanged>
				EpsilonAuraManagerAuraEditor_MarkDirty();
		  </OnTextChanged>
		</Scripts>
	  </EditBox>

	  <ScrollFrame parentKey="buffDesc" name="$parentbuffDesc" inherits="InputScrollFrameTemplate">
		<Size x="250" y="64"/>
		<Anchors>
			<Anchor point="TOPLEFT" relativeKey="$parent.buffIcon" relativePoint="BOTTOMLEFT" x="4" y="-24"/>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentbuffDescTitle" parentKey="Label" inherits="GameFontNormalLeft" justifyH="left" text="Aura Description">
				<Size x="150" y="12"/>
				<Anchors>
				  <Anchor point="TOPLEFT" x="0" y="20"/>
				</Anchors>
			</FontString>
		  </Layer>
		</Layers>
		<KeyValues>
			<KeyValue key="instructions" value="The tooltip description of this aura."/>
			<KeyValue key="hideCharCount" value="true" type="boolean"/>
		</KeyValues>
		<Scripts>
			<OnLoad>
				InputScrollFrame_OnLoad(self);
				self.EditBox:HookScript("OnTextChanged", EpsilonAuraManagerAuraEditor_MarkDirty);
			</OnLoad>
		</Scripts>
	  </ScrollFrame>

	  <ScrollFrame parentKey="spellDesc" name="$parentspellDesc" inherits="InputScrollFrameTemplate">
		<Size x="250" y="64"/>
		<Anchors>
			<Anchor point="TOP" relativeKey="$parent.buffDesc" relativePoint="BOTTOM" x="0" y="-30"/>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
		  <FontString name="$parentSpellDescTitle" parentKey="Label" inherits="GameFontNormalLeft" justifyH="left" text="Spell Description">
				<Size x="150" y="12"/>
				<Anchors>
				  <Anchor point="TOPLEFT" x="0" y="20"/>
				</Anchors>
			</FontString>
		  </Layer>
		</Layers>
		<KeyValues>
			<KeyValue key="instructions" value="The tooltip description of this spell."/>
			<KeyValue key="hideCharCount" value="true" type="boolean"/>
		</KeyValues>
		<Scripts>
			<OnLoad>
				InputScrollFrame_OnLoad(self);
				self.EditBox:HookScript("OnTextChanged", EpsilonAuraManagerAuraEditor_MarkDirty);
			</OnLoad>
		</Scripts>
	  </ScrollFrame>

	  <CheckButton name="$parentBuffVisibility" parentKey="buffVisibility" inherits="ChatConfigCheckButtonTemplate">
			<Anchors>
			  <Anchor point="TOPLEFT" relativeKey="$parent.spellDesc" relativePoint="BOTTOMLEFT" x="-6" y="-10" />
			</Anchors>

			<Scripts>
				<OnLoad>
					_G[self:GetName().."Text"]:SetText("|cFFFFD100Visible")
				</OnLoad>
				<PostClick>
					self:GetScript("OnEnter")(self);
				</PostClick>
				<OnEnter>
					self.status = "|cFFFF0000Not Visible|r";
					if self:GetChecked() then
						self.status = "|cFF00FF00Visible|r";
					end
					GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
					GameTooltip:AddLine( self.status, 1, 1, 1, 1)
					GameTooltip:AddLine("Toggle whether this aura is visible to players while in this phase. Has no effect on spells.", 1, 0.81, 0, 1)
					GameTooltip:Show();
				</OnEnter>
				<OnLeave function="GameTooltip_Hide"/>
			</Scripts>
	  </CheckButton>

	  <CheckButton name="$parentBuffOverride" parentKey="buffOverride" inherits="ChatConfigCheckButtonTemplate">
			<Anchors>
			  <Anchor point="TOPLEFT" relativeKey="$parent.buffVisibility" relativePoint="BOTTOMLEFT" x="0" y="0" />
			</Anchors>

			<Scripts>
				<OnLoad>
					_G[self:GetName().."Text"]:SetText("|cFFFFD100Allow Personal Override")
				</OnLoad>
				<PostClick>
					self:GetScript("OnEnter")(self);
				</PostClick>
				<OnEnter>
					self.status = "|cFFFF0000Disallow|r";
					if self:GetChecked() then
						self.status = "|cFF00FF00Allow|r";
					end
					GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
					GameTooltip:AddLine( self.status .. " Personal Override", 1, 1, 1, 1)
					GameTooltip:AddLine("Toggle whether these edits can be overridden by players with their own personal edits of this spell or aura.|n|n|cFFFFFFFFTip:|r This can be useful if you only want your edits to apply to NPCs in your phase.", 1, 0.81, 0, 1)
					GameTooltip:Show();
				</OnEnter>
				<OnLeave function="GameTooltip_Hide"/>
			</Scripts>
	  </CheckButton>

	  <Button name="$parentSaveButton" inherits="UIPanelButtonTemplate" text="Save">
			<Size x="85" y="22"/>
			<Anchors>
				<Anchor point="BOTTOMLEFT" x="6" y="4"/>
			</Anchors>
			<Scripts>
				<OnClick>
					EpsilonAuraManagerAuraEditor_Save()
				</OnClick>
			</Scripts>
	  </Button>

	  <Button name="$parentCancelButton" inherits="UIPanelButtonTemplate" text="Cancel">
			<Size x="85" y="22"/>
			<Anchors>
				<Anchor point="BOTTOMRIGHT" x="-6" y="4"/>
			</Anchors>
			<Scripts>
				<OnClick>
					EpsilonAuraManagerAuraEditor_Close( true )
				</OnClick>
			</Scripts>
	  </Button>

    </Frames>
  </Frame>

</Ui>
