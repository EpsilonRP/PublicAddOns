						<Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
					<Layers>
						<Layer level="ARTWORK">
							<Texture parentKey="botLeftCorner" inherits="UI-Frame-BotCornerLeft">
