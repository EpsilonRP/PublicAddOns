									<Anchor point="LEFT" x="-7" y="0"/>
									<Anchor point="BOTTOM" x="0" y="-5"/>
								</Anchors>
							</Texture>
							<Texture parentKey="botRightCorner" inherits="UI-Frame-BotCornerRight">
