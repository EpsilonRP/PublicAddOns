	<Frame name="EpsilonAuraManagerAuraEditor" parent="EpsilonAuraManagerFrame" inherits="ButtonFrameTemplate" hidden="true" frameStrata="DIALOG" enableMouse="true" movable="true" topLevel="true">
    <Size x="300" y="420" />
    <Anchors>
      <Anchor point="LEFT" relativeTo="EpsilonAuraManagerFrame" relativePoint="RIGHT" x="0" y="0" />
    </Anchors>

	<Layers>
		<Layer level="OVERLAY">
			<FontString parentKey="Header" inherits="GameFontNormal" justifyH="center" text="Original Aura:">
				<Anchors>
					<Anchor point="TOP" x="0" y="-72"/>
				</Anchors>
			</FontString>
			<Texture parentKey="originalBuffIcon" file="Interface/Icons/inv_misc_questionmark">
				<Size x="21" y="21"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="20" y="-90"/>
				</Anchors>
			</Texture>
			<FontString parentKey="originalBuffName" inherits="GameFontNormal" justifyH="left" maxLines="2" text="Aura Name">
				<Anchors>
				  <Anchor point="LEFT" relativeKey="$parent.originalBuffIcon" relativePoint="RIGHT" x="5" y="0"/>
				  <Anchor point="RIGHT" x="-20" y="0"/>
				</Anchors>
			</FontString>
			<Texture parentKey="Divider" file="Interface/ClassTrainerFrame/UI-ClassTrainer-HorizontalBar">
				<Size x="220" y="12"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="2" y="-115"/>
				</Anchors>
				<TexCoords left="0" right="1.0" top="0" bottom="0.25"/>
			</Texture>
			<Texture file="Interface/ClassTrainerFrame/UI-ClassTrainer-HorizontalBar">
				<Size x="73" y="12"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.Divider" relativePoint="RIGHT" x="0" y="0"/>
				</Anchors>
				<TexCoords left="0" right="0.29296875" top="0.25" bottom="0.5"/>
			</Texture>
			<FontString parentKey="Header2" inherits="GameFontNormal" justifyH="center" text="Your Edits:">
				<Anchors>
					<Anchor point="TOP" x="0" y="-130"/>
				</Anchors>
			</FontString>
		</Layer>
	</Layers>

    <Scripts>
      <OnLoad>
		  self:SetClampedToScreen( true );
		  self:SetMovable(true);
		  self:EnableMouse(true);
		  self:RegisterForDrag( "LeftButton" );
		  self:SetScript( "OnDragStart", self.StartMoving );
		  self:SetScript( "OnDragStop", self.StopMovingOrSizing );
		  self:SetUserPlaced( true );

		  SetPortraitToTexture(self.portrait,"Interface/Icons/misc_rnrpaintbuttonup");

		  self.TitleBgColor = self:CreateTexture(nil, "BACKGROUND")
		  self.TitleBgColor:SetPoint("TOPLEFT", self.TitleBg)
		  self.TitleBgColor:SetPoint("BOTTOMRIGHT", self.TitleBg)
		  self.TitleBgColor:SetColorTexture(2 / 255, 102 / 255, 148 / 255, 0.5)

		  NineSliceUtil.ApplyLayoutByName(self.NineSlice, "EpsilonGoldBorderFrameTemplate")

		  self.TitleText:SetText( "Edit Aura" )
	  </OnLoad>
	  <OnShow>
		EpsilonAuraManagerAuraEditor_Refresh()
	  </OnShow>
    </Scripts>
    <Frames>

      <!-- Search/filter box. -->
		<EditBox parentKey="Search" inherits="SearchBoxTemplate">
			<Size x="200" y="20" />
			<Anchors>
				<Anchor point="TOPLEFT" x="72" y="-38" />
			</Anchors>
			<Layers>
			  <Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontNormalSmall" justifyH="center" text="Search for an aura to edit:">
					<Anchors>
					  <Anchor point="TOP" x="0" y="12"/>
					</Anchors>
				</FontString>
			  </Layer>
			</Layers>
			<Frames>
				<Frame parentKey="searchPreviewContainer" hidden="true" frameStrata="FULLSCREEN_DIALOG">
